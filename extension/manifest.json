{
  "manifest_version": 2,
  "name": "Streak Texting",
  "description": "Send personalized messages to all rows checked in Streak",
  "version": "1.0.5",
  "browser_action": {
    "default_title": "Streak Texting",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*"
      ],
      "js": [
        "js/streak_cs.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "https://www.google.com/voice/*"
      ],
      "js": [
        "js/voice_cs.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "notifications"
  ]
}
